CREATE DATABASE YOUTUBE

CREATE OR REPLACE TABLE PATIENTS (
PATIENTID INT,
FIRST_NAME VARCHAR (100),
CITY VARCHAR (100),
REGISTRATIONYEAR INT
);

CREATE OR REPLACE STAGE PATIENT_SNOW_STAGE
URL='s3://pipelinepatient'
credentials = (aws_key_id = 'AKIAYY5MARGRMY3XDJKZ' aws_secret_key = 'wIiU5QxAjydeWyfLTOaonRUtVfAD2Fh6IwrHr9mA')
FILE_FORMAT = CSV;

SHOW STAGES;

CREATE OR REPLACE PIPE PATIENT_SNOWPIPE AUTO_INGEST = TRUE AS
COPY INTO PATIENTS
FROM @PATIENT_SNOW_STAGE
FILE_FORMAT = CSV;

SHOW PIPES;

SHOW STAGES;

SELECT * FROM PATIENTS;
